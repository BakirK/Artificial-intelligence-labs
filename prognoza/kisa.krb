kabanicu
    foreach
    	vrijeme.kisa(True)
    	vrijeme.vjetar(True)
    assert
    	vrijeme.ponijeti(kabanicu)
kisobran
    foreach
    	vrijeme.kisa(True)
        vrijeme.vjetar(False)
    assert
    	vrijeme.ponijeti(kisobran)
vjetrovka
    foreach     
        vrijeme.kisa(False)
        vrijeme.vjetar(True)
    assert
        vrijeme.ponijeti(vjetrovku)
nista
    foreach     
        vrijeme.vjetar(False)
        vrijeme.kisa(False)
    assert
        vrijeme.ponijeti(nista)